{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true
  },
  "rules": {
    "func-names": ["error", "never"],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["**/example/**/*.js"]}],
    "no-console": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "strict": "off"
  }
}
